/*导入公共样式*/
@import url("public.css");
/*导入页头样式*/
@import url("header.css");
/*导入翻页样式*/
@import url("page_break.css");



.tabseach{ float:left; height:35px; line-height:35px; }


.tabtext{ min-width:978px; border:1px #cecece solid; -moz-border-radius:5px; -webkit-border-radius:5px; background:#fff; }
.scenic_search{ float:left;width:100%;background:#8EEDFE;line-height:40px; padding-left:8px; overflow:hidden;border:1px #c0e3f8 solid; -moz-border-radius:5px; -webkit-border-radius:5px;margin-bottom:5px; max-width: 1380px;}
.chose{float:left;width:auto;color:#252525;line-height:40px;}
.chose span{color:#252525; }
.chose .searchExtend{float:left;line-height:24px;_padding-top:5px;}
.chose .radio{vertical-align:middle;margin-left:10px;margin-right:2px;*margin-right:0;}
.chose .txt{ float:left; background:#fff 0 0 no-repeat; width:207px; height:20px; padding:4px 0px 0 5px; margin-right:0px; border:1px #6A80A9 solid; }
.chose .txtSearch{float:left;width:196px; border:0; background-color:transparent; }
.chose .selSearch{float:left;width:118px;height:24px;line-height:20px; background:#fff; border:1px #6A80A9 solid; margin-right:2px; }
.chose .nowSearch{float:left;width:82px;height:20px; overflow:hidden;padding:3px 5px 0 10px;  }
.chose .btnSel{float:right;width:18px;height:19px; margin-top:3px;margin-right:2px;}
.chose .btnSel a{background:url(../images/index/bg_search_s.jpg) no-repeat center center;display:block;width:17px;height:19px;}
.chose .selOption{z-index:9999;position:absolute; width:118px;background:#f2f2f2;border:1px solid #6A80A9; border-top:0; margin-left:-1px; margin-top:2px; overflow:hidden;}
.chose .selOption a{display:block;height:24px;padding-left:5px;line-height:24px;color:#000;text-decoration:none;}
.chose .selOption a:hover{color:#fff;background:#ccc;}
.chose .selOptionProv{z-index:9999;position:absolute; width:118px;background:#f2f2f2;border:1px solid #6A80A9; border-top:0; margin-left:-1px; margin-top:2px; height:300px; overflow:hidden; overflow-y:scroll; }
.chose .selOptionProv a{display:block;height:24px;padding-left:5px;line-height:24px;color:#000;text-decoration:none;}
.chose .selOptionProv a:hover{color:#fff;background:#ccc;}
.chose .btn_search{float:left;margin-left:4px;width:70px;height:25px; background:url(../images/index/btn_search.jpg) 0 1px no-repeat; overflow:hidden;}
.chose .btn_search a{display:block;text-decoration:none;height:25px;overflow:hidden;}

.scenic_list{ float:left; width:100%;}
table{border-collapse:collapse;  width:100%;}
table.itable{border:1px solid #c4c4c4; margin:0 auto;}
table.itable a { color:#333; text-decoration:none;}
table.itable tr:hover { background:#dcf0f4;}
.itable caption{font-size:1em; font-weight:normal; color:#a40000}
.itable thead{background:url(../images/thead_bg.jpg) repeat-x;height:30px;}
.itable td{padding:4px 0 4px 10px;border:1px solid #ccc;color:#5c5c5c;}
table.itable th{font-weight:bold;border:1px solid #ccc; color:#5c5c5c; background:#eee; border-top:1px solid #fff;}
.itable .linenum{width:15%; background:#fff}
.itable .id-1{width:10%; background:#f8f8f8}
.itable .id-2{width:15%; background:#fff}
.itable .id-3{width:15%; background:#f8f8f8}
.itable .id-4{width:15%; background:#fff}
.itable .id-5{width:auto; background:#f8f8f8}
table.itable img{ vertical-align:middle; margin-top:-3px; margin-right:3px;}
.taboff { color:#333;}
.tabtext a:hover {color:#2089c0;}

.pagebreak{ float:right; margin-top:5px;}
