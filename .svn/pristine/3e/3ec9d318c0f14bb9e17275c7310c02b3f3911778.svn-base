.thumb {
    margin: 15px auto 0px;
    float: right;
}

.thumb a {
    display: inline-block;
    margin: 0px 1px;
    height: 25px;
    line-height: 25px;
    padding: 0px 8px;
    border: 1px solid rgb(184, 184, 184);
    font-weight: bold;
    color: rgb(138, 138, 138);
    text-decoration: none;
}

.thumb .current {
    color: rgb(255, 255, 255);
    background: rgb(27, 172, 209);
}

.thumb a:hover {
    background: rgb(27, 172, 209);
    color: rgb(255, 255, 255);
}

.rnoHeader {
    padding: 0 10px;
    background: url("../images/cmp-bg2.gif") repeat-x;
    color: rgb(68, 68, 68);
    white-space: nowrap;
    border: 1px solid rgb(204, 204, 204);
    line-height: 30px;
}

.rnoHeader th {
    text-align: left;
    padding-left: 28px;
    padding-top: 1px;
    font-weight: bold;
}
.info-style {
    width: 230px;
    height: 56px;
    position: fixed;
    top: 50%;
    left: 45%;
    line-height: 56px;
    color: #fff;
    text-align: center;
    font-size: 15px;
    background: #ff0000;
    filter: alpha(opacity=100);
    -moz-opacity: 1.0;
    opacity: 1.0;
    z-index: 9999;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
}
.loading {
    width: 160px;
    height: 56px;
    position: fixed;
    top: 50%;
    left: 45%;
    line-height: 56px;
    color: #fff;
    padding-left: 60px;
    font-size: 15px;
    background: #000 url("../images/loader.gif") no-repeat 10px 50%;
    opacity: 0.7;
    z-index: 9999;
    -moz-border-radius: 20px;
    -webkit-border-radius: 20px;
    border-radius: 20px;
    filter: progid:DXImageTransform.Microsoft.Alpha(opacity=70);
}