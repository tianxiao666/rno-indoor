body {
    font-size: 12px;
    font-family: ����;
    width: 100%;
    height: 100%;
}

body, h1, h2, h3, h4, h5, h6, dl, dt, dd, ul, ol, form, input, p, strong {
    margin: 0px;
    padding: 0px;
}

h1, h2, h3, h4, h5, h6 {
    font-size: 12px;
    font-weight: normal;
    vertical-align: baseline;
}

ol, ul {
    list-style-type: none;
}

input {
    vertical-align: middle;
}

a {
    cursor: pointer;
    color: rgb(51, 51, 51);
}

.warp {
    width: 100%;
    height: 100%;
    background-color: rgb(255, 255, 255);
}

.crumb {
    line-height: 30px;
    height: 30px;
    margin: 10px;
    border: 1px solid rgb(221, 221, 221);
    padding: 0px 15px;
}

.crumb a {
    color: rgb(51, 51, 51);
    text-decoration: none;
}

.crumb a:hover {
    color: rgb(27, 172, 209);
}

.popup {
    border: 6px solid rgb(238, 238, 238);
    border-radius: 3px;
    color: rgb(102, 102, 102);
    font-style: normal;
    font-variant: normal;
    font-weight: normal;
    font-stretch: normal;
    font-size: 12px;
    line-height: 100%;
    font-family: Arial, Helvetica, Tahoma, sans-serif, 宋体;
    margin: 0px auto;
    text-align: left;
    width: 300px;
    z-index: 99999;
}

.p-box {
    background: rgb(255, 255, 255);
}

.p-title, .p-bom {
    height: 23px;
    line-height: 23px;
    background: rgb(245, 245, 245);
    text-indent: 0.5em;
    position: relative;
}

.p-title {
    border-bottom: 1px solid rgb(229, 229, 229);
}

.p-main {
    margin: 15px;
}

.p-main .content {
    display: none;
}

.p-bom {
    border-top: 1px solid rgb(229, 229, 229);
}

.p-title span, .p-bom span {
    cursor: pointer;
    right: 5px;
    color: rgb(102, 102, 102);
    font-weight: bold;
    font-family: Arial;
    font-size: 12px;
    position: absolute;
}

.p-title .p-title-content {
    float: left;
}

.pop_Content_close {
    background: url("../images/pop_norepeat_bg.gif") 0px 0px repeat scroll transparent;
    display: block;
    height: 23px;
    overflow: hidden;
    text-indent: -9999px;
    width: 22px;
}

.frmPopup {
    width: 100%;
    height: 100%;
    display: none;
}

.popup {
}

.divPopup {
    display: none;
    position: absolute;
}

.tabtext {
    min-width: 978px;
    border: 1px solid rgb(206, 206, 206);
    border-radius: 5px;
    background: rgb(255, 255, 255);
}

.scenic_search {
    float: left;
    width: 100%;
    background: rgb(142, 237, 254);
    line-height: 40px;
    padding-left: 8px;
    overflow: hidden;
    border: 1px solid rgb(192, 227, 248);
    border-radius: 5px;
    margin-bottom: 5px;
    max-width: 1380px;
}

table {
    border-collapse: collapse;
    width: 100%;
}

table.itable {
    border: 1px solid rgb(196, 196, 196);
    margin: 0px auto;
}

table.itable a {
    color: rgb(51, 51, 51);
    text-decoration: none;
}

table.itable tr:hover {
    background: rgb(220, 240, 244);
}

.itable caption {
    font-size: 1em;
    font-weight: normal;
    color: rgb(164, 0, 0);
}

.itable thead {
    background: url("../images/thead_bg.jpg") repeat-x;
    height: 30px;
}

.itable td {
    padding: 4px 0px 4px 10px;
    border: 1px solid rgb(204, 204, 204);
    color: rgb(92, 92, 92);
}

table.itable th {
    font-weight: bold;
    border-width: 1px;
    border-style: solid;
    border-color: rgb(255, 255, 255) rgb(204, 204, 204) rgb(204, 204, 204);
    border-image: initial;
    color: rgb(92, 92, 92);
    background: rgb(238, 238, 238);
}

.itable .linenum {
    width: 15%;
    background: rgb(255, 255, 255);
}

.itable .id-1 {
    width: 10%;
    background: rgb(248, 248, 248);
}

.itable .id-2 {
    width: 15%;
    background: rgb(255, 255, 255);
}

.itable .id-3 {
    width: 15%;
    background: rgb(248, 248, 248);
}

.itable .id-4 {
    width: 15%;
    background: rgb(255, 255, 255);
}

.itable .id-5 {
    width: auto;
    background: rgb(248, 248, 248);
}

table.itable img {
    vertical-align: middle;
    margin-top: -3px;
    margin-right: 3px;
}

.tabtext a:hover {
    color: rgb(32, 137, 192);
}

.pagebreak {
    float: right;
