
.popup{border:6px solid #EEEEEE;border-radius: 3px 3px 3px 3px;color: #666666;font: 12px/100% Arial,Helvetica,Tahoma,sans-serif,宋体; margin: 0 auto;text-align: left;width: 300px;z-index: 99999;}
.p-box{background:#fff;}
.p-title,.p-bom{height:23px;line-height:23px;background:#f5f5f5;text-indent:0.5em;position:relative;}
.p-title{border-bottom:1px solid #e5e5e5;}
.p-main{margin:15px;}
.p-main .content{display:none;}
.p-bom{border-top:1px solid #e5e5e5;}
.p-title span,.p-bom span{cursor:pointer;position:absolute;right:5px;;color:#666;font-weight:bold;font-family:Arial;font-size:12px;position:absolute;}
.p-title .p-title-content{float:left;}
.p-left{padding:0px 15px 10px 15px;float:left;}
.note{margin-left:10px;width:282px;line-height:120%;font-size:12px;height:30px;}
.common{width:285px;margin-left:10px;font-size:12px;}
.common span{float:right;height:27px;line-height:27px;margin-bottom:10px;}
.common-bor{width:276px;padding:3px;border:2px solid #ffefaf;float:left;margin-right:5px;text-align:center;margin-bottom:10px;}
.common-txt{height:18px;line-height:18px;background:#f5f5f5;border:1px solid #b6b6b6;width:283px;text-indent:0.3em;}
.note-rig{float:right;height:30px;line-height:30px;}
.note-rig span{font-size:14px;color:#ff5c00; font-family:"黑体";}
.p-rig{float:right;width:310px;}
.btn{background:url(../images/btn.jpg) no-repeat;border:medium none;color:#FFFFFF;cursor:pointer;float:left;font-size:14px;height:27px;line-height:27px;margin-bottom:10px;text-align:center;width:80px;}
.hidden {display:none;}
.pop_Content_close {
    background: url("../images/pop_norepeat_bg.gif") repeat scroll 0 0 transparent;
    display: block;
    height: 23px;
    overflow: hidden;
    text-indent: -9999px;
    width: 22px;
}
.frmPopup {
	width:100%;
	height:100%;
	display:none;
}
.popup {
	/*position:relative;*/
}
.divPopup {
	/*width:100%;*/
	display:none;
	position:absolute;
}